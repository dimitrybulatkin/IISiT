    <defrule name="expHightDefrule">
        <premise name="work" value="да" />
        <premise name="age" value="да" />
    </defrule>

    <defrule name="expNormalDefrule">
        <premise name="work" value="да" />
        <premise name="age" value="нет" />
    </defrule>

